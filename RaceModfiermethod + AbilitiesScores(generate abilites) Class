            void GenerateMethod3()
            {
                int[] scores1 = new int[5];
                for (int i = 0; i < 5; i++)
                {
                    scores1[i] = random.Next(1, 6); // Min/Max = 5/21
                }
                Array.Sort(scores1);
                int _combinedscores1 = scores1[0] + scores1[1] + scores1[2] + random.Next(1, 3);
                int _finalscore1 = random.Next(1, 6);
                _t1 = _combinedscores1; //

                int[] scores2 = new int[5];
                for (int i = 0; i < 5; i++)
                {
                    scores2[i] = random.Next(1, 6); // Min/Max = 5/21
                }
