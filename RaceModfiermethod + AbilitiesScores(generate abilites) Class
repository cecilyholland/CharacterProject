namespace ClassProject
{
    public class CharacterChoices()
    {
        public static string race;
        public string alignment;
        public int age;
        public string height;
        public int weight;
        private int _count = 0;
        public int choice = Convert.ToInt32(Console.ReadLine);
        Random rnd = new Random();

        public void RaceAlign()
        {
            while (_count != 2)
            { 
                Console.WriteLine("1.Select Alignment \n2.Select Race");
                switch (choice) //Setting switch cases
                {
                    case 1:
                        Console.WriteLine("Choose from of the following: \n1. Lawful Good \n2. Lawful Evil \n3. Neutral Good \n4.Neutral Evil\n5.Chaotic Good\n6.Chaotic Evil");
                        choice = Convert.ToInt32(Console.ReadLine);
                        int _a1 = choice;
                        if (_a1 == 1)
                            alignment = "Lawful Good";
                        if (_a1 == 2)
                            alignment = "Lawful Evil";
                        if (_a1 == 3)
                            alignment = "Neutral Good";
                        if (_a1 == 4)
                            alignment = "Neurtral Evil";
                        if (_a1 == 5)
                            alignment = "Chaotic Good";
                        if (_a1 == 6)
                            alignment = "Chaotic Evil";
                        _count++;
                        continue;

                    case 2:
                        Console.WriteLine("Choose from of the following: \n1. Human \n2. Elf \n3.Dwarf \n4.Gnomel \n5.Halfling");
                        choice = Convert.ToInt32(Console.ReadLine);
                        int _a2 = choice;
                        if (_a2 == 1)
                            //code for code altering attribute values
                            race = "Human";
                            
                        if (_a2 == 2)
                            //code for altering attribute values
                            //Elf – Dex+2, Int+2, Con-2
                            race = "Elf";
                        if (_a2 == 3)
                            //code for altering attribute values
                            //Dwarf – Str + 2, Con + 2, Cha - 2
                            race = "Dwarf";
                        if (_a2 == 4)
                            //code for altering attribute values
                            //Gnome – Dex + 2, Con + 2, Str - 2
                            race = "Gnome";
                        if (_a2 == 5)
                            //code for altering attribute values
                            //Halfling – Dex+2, Wis+2, Str-2
                            race = "Halfling";
                            
                        _count++;
                        break;
                }
            }
        }

        public class AbilityScores
        {
            private Random random;
            Dictionary<string, int> score_card = new Dictionary<string, int>();
            Dictionary<string, int> ability_card = new Dictionary<string, int>();
            int _t1;
            int _t2;
            int _t3;
            int _t4;
            int _t5;
            int _t6;
            public AbilityScores()
            {
                random = new Random();
            }
            public void DisplayScores()
            {
                foreach (var item in score_card)
                {
                    Console.WriteLine("Key " + item.Key + " Value " + item.Value);
                }
            }

            public void DisplayAbilities()
            {
                foreach (var item in ability_card)
                {
                    Console.WriteLine("Key " + item.Key + " Value " + item.Value);
                }
            }

            public void GenerateScores()
            {
                int method;
