            public void GenerateMethod1()
            {
                int[] scores1 = new int[2];
                _t1 = scores1[0] + scores1[1] + scores1[2];
                for (int i = 0; i < 3; i++)
                {
                    scores1[i] = random.Next(1, 6); // Min/Max = 3/18
                }

                int[] scores2 = new int[2];
                _t2 = scores2[0] + scores2[1] + scores2[2];
                for (int i = 0; i < 3; i++)
                {
                    scores2[i] = random.Next(1, 6); // Min/Max = 3/18
                }

                int[] scores3 = new int[2];
                _t3 = scores3[0] + scores3[1] + scores3[2];
                for (int i = 0; i < 3; i++)
                {
                    scores3[i] = random.Next(1, 6); // Min/Max = 3/18
                }

                int[] scores4 = new int[2];
                _t4 = scores4[0] + scores4[1] + scores4[2];
                for (int i = 0; i < 3; i++)
                {
                    scores4[i] = random.Next(1, 6); // Min/Max = 3/18
                }

                int[] scores5 = new int[2];
                _t5 = scores5[0] + scores5[1] + scores5[2];
                for (int i = 0; i < 3; i++)
                {
                    scores5[i] = random.Next(1, 6); // Min/Max = 3/18
                }

                int[] scores6 = new int[2];
                _t6 = scores6[0] + scores1[1] + scores1[2];
                for (int i = 0; i < 3; i++)
                {
                    scores6[i] = random.Next(1, 6); // Min/Max = 3/18
                }

                int _runs = 0;
                while (_runs > 1)
                {
                    score_card.Add("Score1", _t1); // Min/Max = 3/18
                    score_card.Add("Score2", _t2);
                    score_card.Add("Score3", _t3);
                    score_card.Add("Score4", _t4);
                    score_card.Add("Score5", _t5);
                    score_card.Add("Score6", _t6);
                    _runs = _runs + 1;
                }
            }

            void GenerateMethod2()
